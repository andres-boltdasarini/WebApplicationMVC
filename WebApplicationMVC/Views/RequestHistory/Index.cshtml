@{
    ViewData["Title"] = "Request History";
}

<h1>@ViewData["Title"]</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Date</th>
            <th>URL</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var log in Model)
        {
            <tr>
                <td>@log.Id</td>
                <td>@log.Date.ToString("yyyy-MM-dd HH:mm:ss")</td>
                <td>@log.Url</td>
            </tr>
        }
    </tbody>
</table>